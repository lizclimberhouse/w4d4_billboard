<div class = "spacy_space container">
  <h1>Select an Artist To View Details</h1>
  <hr />
</div>

<a class = "ui button blue" href="/billboards">Go to Billboards list</a>
<div class="spacy_space">
    <div class="ui four cards olive">
      <% @artists.each do |artist| %>
        <div class = "ui  card">
          <div class="content centered">
            <%= link_to artist.name, artist%>
          </div>
        </div>
      <% end %>
    </div>
</div>

<a class = "ui button blue" href="/artists/new">Add a New Artist</a>
